open System

let countFloors input =
    let rec floorLoop floorNumber input =
        match input with
        | '('::input -> floorLoop (floorNumber+1) input
        | ')'::input -> floorLoop (floorNumber-1) input
        | _ -> floorNumber

    floorLoop 0 (input|>Seq.toList)

let testFloors input =
    sprintf "%s -> floor %d" input (countFloors input)
    |> Console.WriteLine

